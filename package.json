{
  "name": "parkhere",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start --dev-client",
    "start:offline": "npx expo start --dev-client --offline",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
